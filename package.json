{
  "name": "configuratore-bootstrap-italia",
  "version": "1.0.0",
  "description": "Example on using create-react-app with a Node Express Backend",
  "author": {
    "name": "Fabio Fumis",
    "twitter": "@FFUmis"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon --ignore 'public/*' server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server.js"
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-select": "^1.13.12",
    "colr": "^1.2.2",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.19",
    "node-fetch": "^2.6.0",
    "node-sass-promise": "^1.0.0",
    "node-wget-promise": "^0.1.6",
    "owl.carousel": "^2.3.4",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
